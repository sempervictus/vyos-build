+      }
+      t_dentry = path.dentry;
+    if (!t_dentry->d_inode)
+      {
+        err = -RSBAC_EINVALIDTARGET;
+        goto out_dput;
+      }
+    /* is inode of type file? */
+    if(S_ISREG(t_dentry->d_inode->i_mode))
+      target = T_FILE;
+    else
+    if(S_ISDIR(t_dentry->d_inode->i_mode))
+      target = T_DIR;
