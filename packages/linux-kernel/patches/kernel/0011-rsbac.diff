+          case ACLC_remove_from_acl_entry:
+            err = rsbac_acl_sys_remove_from_acl_entry(ta_number,
+                                                      k_arg.target,
+                                                      tid,
+                                                      k_arg.subj_type,
+                                                      k_arg.subj_id,
+                                                      k_arg.rights);
+            break;
+          case ACLC_set_mask:
+            err = rsbac_acl_sys_set_mask(ta_number,
+                                         k_arg.target,
+                                         tid,
+                                         k_arg.rights);
+            break;
+
+          default:
+            err = -RSBAC_EINVALIDREQUEST;
+        }
+
+out_dput:
+      if(k_arg.name)
+        {
+          path_put(&path);
+        }
+
+out:
+      return err;
+    }      /* end of sys_rsbac_acl_n() */
