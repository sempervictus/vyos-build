+							    rc, 0,
+							    &tid_p->user,
+							    &aci);
+			}
+		}
+		break;
+#endif
+
+#if defined(CONFIG_RSBAC_AUTH)
+	case SW_AUTH:
+		{
+			rsbac_system_role_int_t role =
+			    value_p->system_role;
+
+			switch (attr) {
+			case A_system_role:
+			case A_auth_role:
