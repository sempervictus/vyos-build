+	rsbac_attribute_value.dummy = 0;
+
+	if (!rsbac_adf_request(R_MODIFY_SYSTEM_DATA,
+				task_pid(current),
+				T_NETDEV,
+				rsbac_target_id,
+				A_none,
+				rsbac_attribute_value))
+		return -EPERM;
+#endif
+
 	dev_load(net, iwr->ifr_name);
 	rtnl_lock();
 	ret = wireless_process_ioctl(net, iwr, cmd, info, standard, private);
