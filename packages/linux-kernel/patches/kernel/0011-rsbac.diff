+                                                    target,
+                                                    tid,
+                                                    new_target,
+                                                    new_tid,
+                                                    attr,
+                                                    attr_val,
+                                                    owner);
+            if(suberror)
+              error = suberror;
+          }
+        item_p = item_p->next;
