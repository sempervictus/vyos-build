+		return -RSBAC_ENOTINITIALIZED;
+	}
+	if (in_interrupt()) {
+		rsbac_printk(KERN_WARNING "rsbac_mac_add_to_p_truset(): called from interrupt!\n");
+	}
+	return rsbac_ta_list_lol_subadd_ttl(ta_number, process_handle, ttl,
+					    &pid, &member, NULL);
+}
