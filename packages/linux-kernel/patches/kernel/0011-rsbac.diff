+                /* shifts work correctly! */
+                log_level =   ((i_attr_val.log_array_low   >> request) & 1)
+                          | ( ((i_attr_val2.log_array_high >> request) & 1) << 1);
+                if (   log_level == LL_full
+                    || (   log_level == LL_denied
+                        && (result == NOT_GRANTED
