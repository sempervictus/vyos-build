+/* Provide means for adding and removing of capabilities */
+int sys_rsbac_mac_add_p_tru(
+  rsbac_list_ta_number_t ta_number,
+  rsbac_upid_t upid,
+  rsbac_uid_t uid,
+  rsbac_time_t ttl)
+  {
