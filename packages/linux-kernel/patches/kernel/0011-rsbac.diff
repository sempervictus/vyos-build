+int rsbac_um_get_user_item(rsbac_list_ta_number_t ta_number,
+			   rsbac_uid_t user,
+			   enum rsbac_um_mod_t mod,
+			   union rsbac_um_mod_data_t *data_p)
+{
+	int err = 0;
+	struct rsbac_um_user_entry_t *entry_p = NULL;
+
+	if (!rsbac_is_initialized()) {
