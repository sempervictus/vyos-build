+						    seq_printf(m,
+							    "%u:%u ",
+							    RSBAC_UID_NUM(cap_list[j].first),
+							    RSBAC_UID_NUM(cap_list[j].last));
+					} else {
+#ifdef CONFIG_RSBAC_UM_VIRTUAL
+						if (RSBAC_UID_SET(cap_list[j].first))
+						    seq_printf(m,
+							    "%u/%u ",
+							    RSBAC_UID_SET(cap_list[j].first),
+							    RSBAC_UID_NUM(cap_list[j].first));
+						else
+#endif
+						    seq_printf(m,
+							    "%u ",
+							    RSBAC_UID_NUM(cap_list[j].first));
+					}
+					}
+					rsbac_kfree(cap_list);
+					all_member_count +=
+					    member_count;
+				}
+			}
+			rsbac_kfree(f_list);
+			all_count += count;
+		}
+		seq_printf(m,
+			    "\ndevice %02u:%02u has %lu file group eff cap set items, sum of %lu members, list is clean\n",
+			    device_p->major, device_p->minor,
+			    all_count,
+			    all_member_count);
+		all_member_count = 0;
+		all_count = 0;
+		count = rsbac_list_lol_get_all_desc(device_p->
+						group_fs_handle,
+						(void **) &f_list);
+		if (count > 0) {
+			for (i = 0; i < count; i++) {
+				member_count =
+				    rsbac_list_lol_get_all_subdesc
+				    (device_p->group_fs_handle,
+				     &f_list[i],
+				     (void **) &cap_list);
+				seq_printf(m,
+					    "\n %u\t%u\t",
+					    f_list[i],
+					    member_count);
+				if (member_count > 0) {
+					for (j = 0;
+					     j < member_count;
+					     j++) {
+					if (cap_list[j].first !=
+					    cap_list[j].last) {
+					    
+#ifdef CONFIG_RSBAC_UM_VIRTUAL
+						if (RSBAC_UID_SET(cap_list[j].first)
+						    || RSBAC_UID_SET(cap_list[j].last)
+						   )
+						    seq_printf(m,
+							    "%u/%u:%u/%u ",
+							    RSBAC_UID_SET(cap_list[j].first),
+							    RSBAC_UID_NUM(cap_list[j].first),
+							    RSBAC_UID_SET(cap_list[j].last),
+							    RSBAC_UID_NUM(cap_list[j].last));
+						else
+#endif
+						    seq_printf(m,
+							    "%u:%u ",
+							    RSBAC_UID_NUM(cap_list[j].first),
+							    RSBAC_UID_NUM(cap_list[j].last));
+					} else {
+#ifdef CONFIG_RSBAC_UM_VIRTUAL
+						if (RSBAC_UID_SET(cap_list[j].first))
+						    seq_printf(m,
+							    "%u/%u ",
+							    RSBAC_UID_SET(cap_list[j].first),
+							    RSBAC_UID_NUM(cap_list[j].first));
+						else
+#endif
+						    seq_printf(m,
+							    "%u ",
+							    RSBAC_UID_NUM(cap_list[j].first));
+					}
+					}
+					rsbac_kfree(cap_list);
+					all_member_count +=
+					    member_count;
+				}
+			}
+			rsbac_kfree(f_list);
+			all_count += count;
+		}
+		seq_printf(m,
+			    "\ndevice %02u:%02u has %lu file group fs cap set items, sum of %lu members, list is clean\n",
+			    device_p->major, device_p->minor,
+			    all_count,
+			    all_member_count);
+#endif
+#endif				/* AUTH_GROUP */
+
+		device_p = srcu_dereference(device_p->next, &device_list_srcu);
+	}
+	srcu_read_unlock(&device_list_srcu, srcu_idx);
+
+	return 0;
+}
