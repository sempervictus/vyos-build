+      {
+        printk(KERN_WARNING "rsbac_write_exec_allowed(): called from interrupt: pid %u(%s)!\n",
+                     current->pid, current->comm);
+        dump_stack();
+        return 1;
+      }
+#ifdef CONFIG_RSBAC_SWITCH_MPROTECT
+    if (!rsbac_switch_mprotect)
