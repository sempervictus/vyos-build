+				  &(device_p->fd_cache_handle[SW_GEN]), list_info_p,
+				  RSBAC_LIST_DEF_DATA | RSBAC_LIST_OWN_SLAB | \
+				    RSBAC_LIST_AUTO_HASH_RESIZE | \
+				    RSBAC_LIST_NO_MAX_WARN,
+				  NULL,
+				  NULL, /* subcompare */
+				  NULL, NULL, /* get_conv */
+				  NULL, NULL, /* def data */
