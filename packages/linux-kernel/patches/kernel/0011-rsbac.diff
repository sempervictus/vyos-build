+static inline rsbac_boolean_t jail_dev_tty(struct rsbac_dev_desc_t dev)
+{
+	if (dev.type != D_char)
+		return FALSE;
+	if (((dev.major >= 2)
+	     && (dev.major <= 4)
+	    )
+	    || ((dev.major >= 128)
+		&& (dev.major <= 143)
+	    )
+	    )
+		return TRUE;
+	else
+		return FALSE;
+}
