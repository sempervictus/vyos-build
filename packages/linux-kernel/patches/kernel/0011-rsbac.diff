+						       &old_rights)) {
+			old_rights &= ~rights;
+			err = rsbac_ta_list_lol_subadd_ttl(ta_number,
+							   device_p->handle,
+							   ttl,
+							   &inode_nr,
+							   &desc,
+							   &old_rights);
+		} else
+			err = 0;
