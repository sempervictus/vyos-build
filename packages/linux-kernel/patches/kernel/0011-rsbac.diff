+                    return(NOT_GRANTED);
+
+                default:
+                  return(DO_NOT_CARE);
+              }
+
+        case R_MODIFY_SYSTEM_DATA:
+            switch(target)
+              {
+                case T_SCD:
+                  switch(tid.scd)
+                    {
+                      case ST_rsbac_log:
