+            err = -EINVAL;
+            goto out;
+          }
+      }
+/* Boolean switch for RSBAC soft mode */
+    /*
+     * Usage: echo "debug softmode #N" > /proc/rsbac_info/debug
+     *   to set rsbac_softmode to given value
+     */
