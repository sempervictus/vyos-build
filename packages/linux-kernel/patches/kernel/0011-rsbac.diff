+#ifdef CONFIG_RSBAC_SOFTMODE
+                }
+#endif
+              }
+            if (program_name)
+              rsbac_kfree(program_name);
+          }
+#endif
+
+#ifdef CONFIG_RSBAC_SOFTMODE
+          if(   !rsbac_softmode
+#ifdef CONFIG_RSBAC_SOFTMODE_IND
+             && !rsbac_ind_softmode[SW_MPROTECT]
