+    tid.file.inode  = t_dentry->d_inode->i_ino;
+    tid.file.dentry_p = t_dentry;
+
+    err = rsbac_mac_add_f_tru(ta_number, tid.file, uid, ttl);
+
+out_dput:
+    path_put(&path);
+
+out:
+    return err;
+  }
