+        }
+      } else if ((vma->rsbac_mprotect_once == 1) && (prot == (PROT_READ | PROT_EXEC))) {
+#ifdef CONFIG_RSBAC_DEBUG
+        if (rsbac_debug_mprotect)
+          {
+            char * program_name = rsbac_get_program_name();
+
