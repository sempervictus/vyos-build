+	rcu_assign_pointer(list->hashed[hash].curr, NULL);
+	rcu_assign_pointer(list->hashed[hash].head, NULL);
+	rcu_assign_pointer(list->hashed[hash].tail, NULL);
+	list->hashed[hash].count = 0;
+	rcu_free_lol_item_chain(list, hash, item_p);
