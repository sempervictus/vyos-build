+			switch (attr) {
+			case A_rc_type_fd:
+				if (aci.rc_type_fd != value_p->rc_type_fd) {
+#ifdef CONFIG_RSBAC_FD_CACHE
+					if (target == T_DIR)
+						need_flush = 1;
