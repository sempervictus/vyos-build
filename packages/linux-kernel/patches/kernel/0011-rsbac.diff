+
+/**********************************************/
+/* Capability lists                           */
+/**********************************************/
+
+#define RSBAC_AUTH_LIST_KEY 626281
+
+#define RSBAC_AUTH_P_LIST_VERSION 1
+#define RSBAC_AUTH_P_LIST_NAME "authproc"
+#define RSBAC_AUTH_P_EFF_LIST_NAME "authproceff"
+#define RSBAC_AUTH_P_FS_LIST_NAME "authprocfs"
