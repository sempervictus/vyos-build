+		       rsbac_acl_rights_vector_t * mask_p)
+{
+	int err = 0;
+	struct rsbac_acl_device_list_item_t *device_p;
+	int srcu_idx;
+	rsbac_old_inode_nr_t inode_nr;
+
+	if (!rsbac_is_initialized()) {
+		rsbac_printk(KERN_WARNING "rsbac_acl_get_mask(): RSBAC not initialized\n");
