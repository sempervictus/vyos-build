+char * inttostr(char * str, int i)
+  {
+    int j = 0;
+    
+    if(!str)
+      return(NULL);
+      
+    if (i<0)
+      {
+        str[j] = '-';
+        j++;
+        i = -i;
+      }
+    if (i>=10000)
+      {
+        str[j] = '0' + (i / 10000);
+        j++;
+      }
+    if (i>=1000)
+      {
+        str[j] = '0' + ((i % 10000) / 1000);
+        j++;
+      }
+    if (i>=100)
+      {
+        str[j] = '0' + ((i % 1000) / 100);
+        j++;
+      }
+    if (i>=10)
+      {
+        str[j] = '0' + ((i % 100) / 10);
+        j++;
+      }
+    str[j] = '0' + (i % 10);
+    j++;
+    str[j] = 0;
+    return (str);
+  };
+
+char * ulongtostr(char * str, u_long i)
+  {
+    int    j = 0;
+    u_long k = 1000000000;
+    
+    if(!str)
+      return(NULL);
+
+    if (i>=k)
+      {
+        str[j] = '0' + ((i / k) % 100);
+        j++;
+      }
+    k /= 10;
+
+    while (k>1)
+      {
+        if (i>=k)
+          {
+            str[j] = '0' + ((i % (k*10)) / k);
+            j++;
+          }
+        k /= 10;
+      };
+
+    str[j] = '0' + (i % 10);
+    j++;
+    str[j] = 0;
+    return (str);
+  };
