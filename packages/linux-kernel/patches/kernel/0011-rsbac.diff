+							     tmp_count, list->hashed[i].count);
+					}
+					item_p = item_p->next;
+				}
+			}
+			if ((tmp_count > 0) && (tmp_count != list->hashed[i].count)) {
+				if (correct) {
+					rsbac_printk(KERN_WARNING "rsbac_check_lists(): correcting count mismatch for list %s hash %u on device %02u:%02u - was %u, counted %lu!\n",
+						     list->name, i,
