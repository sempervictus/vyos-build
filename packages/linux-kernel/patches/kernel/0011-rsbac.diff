+				break;
+			case A_mac_initial_categories:
+				aci.mac_owner_initial_categories =
+				    value_p->mac_categories;
+				break;
+			case A_mac_min_categories:
+				aci.mac_owner_min_categories =
+				    value_p->mac_categories;
+				break;
+			case A_current_sec_level:
+				aci.current_sec_level =
