+						       default_p_handle,
+						       (void **) entry_pp,
+						       ttl_pp);
+
+#ifdef CONFIG_RSBAC_ACL_UM_PROT
+	case T_GROUP:
+		if (tid.group == RSBAC_NO_GROUP)
+			return rsbac_ta_list_get_all_items_ttl(ta_number,
+							       default_g_handle,
