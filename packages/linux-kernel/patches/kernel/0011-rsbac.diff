+      switch(call)
+        {
+          case ACLC_set_acl_entry:
+            err = rsbac_acl_sys_set_acl_entry(ta_number,
+                                              k_arg.target,
+                                              tid,
+                                              k_arg.subj_type,
+                                              k_arg.subj_id,
+                                              k_arg.rights,
+                                              k_arg.ttl);
+            break;
+          case ACLC_remove_acl_entry:
+            err = rsbac_acl_sys_remove_acl_entry(ta_number,
+                                                 k_arg.target,
+                                                 tid,
+                                                 k_arg.subj_type,
+                                                 k_arg.subj_id);
+            break;
+          case ACLC_remove_acl:
+            err = rsbac_acl_sys_remove_acl(ta_number,
+                                           k_arg.target,
+                                           tid);
