+int rsbac_get_adf_log(rsbac_adf_request_int_t request,
+                      enum rsbac_target_t target,
+                      u_int * value_p)
+  {
+    if(   (request < R_NONE)
+       && (target <= T_NONE)
+      )
+      {
+        *value_p = rsbac_log_levels[request][target];
+        return 0;
+      }
+    else
+      return -RSBAC_EINVALIDVALUE;
+  }
