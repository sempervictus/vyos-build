+    switch(dev.type)
+      {
+        case D_block:
+        case D_char:
+          sprintf(str, "%c%u:%u", 'b' + dev.type, dev.major, dev.minor);
+          break;
+        case D_block_major:
+        case D_char_major:
+          sprintf(str, "%c%u",
+                  'b' + dev.type - (D_block_major - D_block),
+                  dev.major);
+          break;
+        default:
+          sprintf(str, "invalid!");
+      }
+    return str;
+  }
