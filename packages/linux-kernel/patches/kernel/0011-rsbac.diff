+#if CONFIG_RSBAC_RCU_RATE > 1000
+#define RCURATE 1000
+#else
+#define RCURATE CONFIG_RSBAC_RCU_RATE
+#endif
+#endif
+u_int rsbac_list_rcu_rate = RCURATE;
+static u_int rcu_callback_count = 0;
