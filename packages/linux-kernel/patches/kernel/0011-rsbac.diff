+
+	if (unlikely(!name))
+		return NULL;
+	/* if there is no current item or it is not the right one, search... */
+	if (!curr || (strncmp(curr->name, name, RSBAC_LIST_MAX_FILENAME)
+		  || (curr->minor != minor)
+		  || (curr->major != major)
+	    )
