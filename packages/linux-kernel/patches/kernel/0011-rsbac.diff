+                           enum rsbac_target_t target,
+                           rsbac_enum_t value)
+  {
+    if(   (request < R_NONE)
+       && (target <= T_NONE)
+       && (value <= LL_full)
+      )
+      {
+        rsbac_log_levels[request][target] = value;
+        if(log_levels_handle)
+          rsbac_list_add(log_levels_handle, &request, rsbac_log_levels[request]);
+      }
+  };
