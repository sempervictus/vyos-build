+			if (!err) {
+				err = rsbac_ta_list_add_ttl(ta_number,
+							    process_handles.gen,
+							    0,
+							    &tid_p->
+							    process, &aci);
+			}
+		}
+		break;
+
+#if defined(CONFIG_RSBAC_MAC)
+	case SW_MAC:
+		{
+			struct rsbac_mac_process_aci_t aci =
+			    DEFAULT_MAC_P_ACI;
+
+			rsbac_ta_list_get_data_ttl(ta_number,
+						   process_handles.mac,
+						   NULL, &tid_p->process,
+						   &aci);
+			switch (attr) {
+			case A_security_level:
+				aci.owner_sec_level =
