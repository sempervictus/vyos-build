+    while(item_p)
+      {
+        if(   item_p->entry.set_attr_func
+        #ifdef CONFIG_RSBAC_SWITCH_REG
+           && item_p->entry.switch_on
+        #endif
+          )
+          {
+            suberror = item_p->entry.set_attr_func (request,
