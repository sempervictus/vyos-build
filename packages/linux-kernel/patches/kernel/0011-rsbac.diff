+			     (void **) entry_pp, ttl_pp);
+#endif
+
+#if defined(CONFIG_RSBAC_ACL_NET_DEV_PROT)
+	case T_NETDEV:
+		if (!tid.netdev[0])
+			return rsbac_ta_list_get_all_items_ttl(ta_number,
+							       default_netdev_handle,
+							       (void **)
+							       entry_pp,
+							       ttl_pp);
