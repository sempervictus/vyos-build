+static const struct proc_ops udf_checker_proc_ops = {
+       .proc_open	= udf_checker_proc_open,
+       .proc_read	= seq_read,
+       .proc_write	= udf_checker_proc_write,
+       .proc_lseek	= seq_lseek,
+       .proc_release	= single_release,
+};
