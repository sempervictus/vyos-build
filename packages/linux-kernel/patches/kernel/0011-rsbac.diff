+						       default_ipc_handle,
+						       (void **) entry_pp,
+						       ttl_pp);
+
+	case T_SCD:
+		if ((tid.scd == AST_none)
+		    || (tid.scd == ST_none)
+		    )
+			return rsbac_ta_list_get_all_items_ttl(ta_number,
+							       default_scd_handle,
+							       (void **)
+							       entry_pp,
+							       ttl_pp);
+		else
+			return
+			    rsbac_ta_list_lol_get_all_subitems_ttl
+			    (ta_number, scd_handle, &tid.scd,
+			     (void **) entry_pp, ttl_pp);
+
+	case T_USER:
+		if (tid.user == RSBAC_NO_USER)
+			return rsbac_ta_list_get_all_items_ttl(ta_number,
+							       default_u_handle,
+							       (void **)
+							       entry_pp,
+							       ttl_pp);
+		else
+			return
+			    rsbac_ta_list_lol_get_all_subitems_ttl
