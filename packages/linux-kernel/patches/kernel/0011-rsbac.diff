+						   &tid_p->user, &aci);
+			switch (attr) {
+			case A_system_role:
+			case A_cap_role:
+				aci.cap_role = value_p->system_role;
+				break;
+			case A_min_caps:
+				aci.min_caps.cap[0] = value_p->min_caps.cap[0];
+				aci.min_caps.cap[1] = value_p->min_caps.cap[1];
