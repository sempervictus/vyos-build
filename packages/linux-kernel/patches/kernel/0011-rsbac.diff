+int rsbac_kthread_notify(rsbac_pid_t pid)
+{
+	if (!rsbac_initialized)
+		return 0;
+//	rsbac_printk(KERN_DEBUG "rsbac_kthread_notify: marking pid %u!\n",
+//		     pid);
+/* Set process aci */
+#ifdef CONFIG_RSBAC_MAC
