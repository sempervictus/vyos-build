+                   && !rsbac_ind_softmode[SW_ACL]
+                #endif
+                  )
+                #endif
+                  return -EPERM;
+               }
+           }
+      }
+
+    /* OK, check passed. Set ACL. */
+    err = rsbac_acl_remove_acl_entry(ta_number, target, tid, subj_type, subj_id);
+    if(err)
+      {
+        char * tmp = rsbac_kmalloc_unlocked(RSBAC_MAXNAMELEN);
