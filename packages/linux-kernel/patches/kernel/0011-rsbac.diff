+  rsbac_res_limit_t k_value;
+  rsbac_time_t k_ttl;
+  struct path path;
+  int err;
+
+  if (!t_name || !value_p)
+    return -RSBAC_EINVALIDPOINTER;
+  switch (attr)
+    {
+      case A_res_min:
+      case A_res_max:
+        break;
+      default:
+        return -RSBAC_EINVALIDATTR;
+    }
+
+  /* lookup filename */
+  if ((err = user_path_at(AT_FDCWD, t_name, 0, &path)))
